        "##Balanceo de Clases"
      ],
      "metadata": {
        "id": "kd5SFrUbC7hw"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "df_cod['gastos_totales'] = df_cod['gastos_totales'].fillna(df_cod['gastos_totales'].median())\n",
        "\n",
        "X = df_cod.drop(columns='estadia_empresa', axis = 1)\n",
        "y = df_cod['estadia_empresa']"
      ],
      "metadata": {
        "id": "Wb0OZYZbGcSM"
